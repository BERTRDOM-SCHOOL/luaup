hide LOAD BAR EXAMPLE
gbl size = 17
test = {"█","██","███","████","█████"}
optable = {}
cnt = 0

readme = readfile("test.txt")
disp(readme sort date)

wait(3)

whilelp
  if cnt < gbl size then
    cnt = cnt + 1
    tinsert(optable, test[random(1, #test)])
    clearconsole
    disp("|" sort tconcat(optable) sort "|")
    wait(.09)
  else
    ret
  end
end
